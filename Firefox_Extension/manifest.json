{
    "manifest_version": 2,
    "name": "WebShield",
    "version": "1.0",
  
    "description": "Adds a red border to all webpages matching mozilla.org.",
  
    "icons": {
      "48": "icons/logo.jpg"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.mozilla.org/*"],
        "js": ["webshield.js"]
      }
    ]
  }
  